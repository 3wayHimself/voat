    <file>/Graphics/arrow-downvoted.png</file>
    <file>/Graphics/arrow-upvote.png</file>
    <file>/Graphics/arrow-upvoted.png</file>
    <file>/Graphics/expand-selfpost-hover.png</file>
    <file>/Graphics/expand-selfpost.png</file>
    <file>/Graphics/thumb-placeholder.png</file>
    <file>/Graphics/whoaverse-mascot.png</file>
    <file>/Graphics/whoaverse.png</file>
	<file>/Graphics/mail-icon.png</file>
  </files>
</sprite>